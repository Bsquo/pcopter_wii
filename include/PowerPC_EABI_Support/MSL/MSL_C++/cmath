#ifndef MSL_CPP_CMATH_H
#define MSL_CPP_CMATH_H
#include <math.h>

namespace std {
    using ::double_t;
    using ::float_t;

    float fabs(float f);

    using ::fmod;

    using ::exp;
    using ::log;
    using ::log10;

    using ::pow;
    using ::sqrt;

    float sin(float f);

    float cos(float f);

    using ::tan;
    float asin(float f);
    using ::acos;
    using ::atan;
    using ::atan2;

    using ::sinh;
    using ::cosh;
    using ::tanh;

    using ::ceil;
    using ::floor;

    using ::frexp;
    using ::ldexp;
    using ::modf;

    float ceilf(float);
    float floorf(float);
    float fmodf(float, float);
    float modff(float, float*);
    float tanf(float);
}

#endif
